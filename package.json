{
	"name": "vue-graybox",
	"version": "0.1.9",
	"main": "dist/vue-graybox.umd.js",
	"module": "dist/vue-graybox.esm.js",
	"unpkg": "dist/vue-graybox.min.js",
	"browser": {
		"./sfc": "src/vue-graybox.vue"
	},
	"scripts": {
		"serve": "vue-cli-service serve",
		"lint": "vue-cli-service lint",
		"build": "vue-cli-service build",
		"build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue-graybox.umd.js",
		"build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-graybox.esm.js",
		"build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vue-graybox.min.js"
	},
	"dependencies": {
		"vue": "^2.5.22",
		"vuex": "^3.0.1"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^3.7.0",
		"@vue/cli-plugin-eslint": "^3.7.0",
		"@vue/cli-service": "^3.7.0",
		"@vue/eslint-config-prettier": "^4.0.1",
		"babel-eslint": "^10.0.1",
		"eslint": "^5.8.0",
		"eslint-plugin-vue": "^5.0.0",
		"rollup": "^0.57.1",
		"rollup-plugin-buble": "^0.19.2",
		"rollup-plugin-vue": "^3.0.0",
		"vue-template-compiler": "^2.5.21"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"@vue/prettier"
		],
		"rules": {},
		"parserOptions": {
			"parser": "babel-eslint"
		}
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not ie <= 8"
	]
}
